<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="NCF_DOM_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <!-- Report Content for NCF_DOM -->
                </div>
            </t>
        </t>
    </template>

    <report 
        id="NCF_DOM_report"
        model="NCF_DOM.model"
        string="Sample Report"
        report_type="qweb-pdf"
        name="NCF_DOM_report_template"
        file="NCF_DOM_report_template"
        attachment_use="False"
    />
</odoo>
